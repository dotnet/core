{
  "description": "SDK download information with direct links to installation files",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "kind": {
      "description": "Type of release document, always \u0027sdk-download\u0027 for SDK download indexes",
      "enum": [
        "releases-index",
        "major-version-index",
        "patch-version-index",
        "sdk-index",
        "manifest",
        "band",
        "sdk-download",
        "downloads-index",
        "component-download",
        "content",
        "unknown",
        "release-index",
        "index",
        "major-index",
        "major-release-index",
        "patch-index",
        "patch-release-index",
        "major-release",
        "patch-release"
      ]
    },
    "version": {
      "description": "SDK version (e.g., \u00278.0.1xx\u0027)",
      "type": "string"
    },
    "support_phase": {
      "description": "Support phase (preview, go-live, active, maintenance, eol)",
      "enum": [
        "preview",
        "go-live",
        "active",
        "maintenance",
        "eol",
        null
      ]
    },
    "title": {
      "description": "Concise title for the document",
      "type": "string"
    },
    "description": {
      "description": "Description of the SDK download",
      "type": "string"
    },
    "_links": {
      "description": "HAL\u002BJSON links for navigation",
      "type": "object",
      "additionalProperties": {
        "description": "HAL\u002BJSON hypermedia link providing navigation to related resources",
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "href": {
            "description": "Absolute URL to the linked resource",
            "type": "string"
          },
          "title": {
            "description": "Descriptive title for the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "type": {
            "description": "MIME type of the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "templated": {
            "description": "Indicates that the href is a URI template (RFC 6570)",
            "type": [
              "boolean",
              "null"
            ]
          }
        },
        "required": [
          "href"
        ]
      }
    },
    "_embedded": {
      "description": "Embedded SDK downloads organized by RID for direct lookup",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "downloads": {
          "description": "Dictionary of SDK downloads keyed by RID (e.g., \u0027linux-x64\u0027, \u0027win-x64\u0027)",
          "type": "object",
          "additionalProperties": {
            "description": "Individual SDK download file for a specific platform",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "name": {
                "description": "File name",
                "type": "string"
              },
              "rid": {
                "description": "Runtime identifier",
                "type": "string"
              },
              "os": {
                "description": "Operating system",
                "type": "string"
              },
              "arch": {
                "description": "Architecture",
                "type": "string"
              },
              "hash_algorithm": {
                "description": "Hash algorithm used for file verification (e.g., \u0027sha512\u0027)",
                "type": "string"
              },
              "_links": {
                "description": "Download and hash links",
                "type": "object",
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "name",
              "rid",
              "os",
              "arch",
              "hash_algorithm",
              "_links"
            ]
          }
        }
      },
      "required": [
        "downloads"
      ]
    }
  },
  "required": [
    "kind",
    "version",
    "support_phase",
    "title",
    "description",
    "_links"
  ]
}