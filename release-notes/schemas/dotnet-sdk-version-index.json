{
  "description": "Index of .NET SDK releases organized by feature bands, supporting navigation from major versions to specific SDK releases",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "kind": {
      "description": "Type of release document, always \u0027index\u0027 for SDK indexes",
      "enum": [
        "releases-index",
        "major-version-index",
        "patch-version-index",
        "sdk-index",
        "manifest",
        "band",
        "content",
        "unknown",
        "release-index",
        "index",
        "major-index",
        "major-release-index",
        "patch-index",
        "patch-release-index",
        "major-release",
        "patch-release"
      ]
    },
    "component": {
      "description": "Component type, always \u0027sdk\u0027 for SDK indexes",
      "type": "string"
    },
    "version": {
      "description": "SDK version (e.g., \u00278.0\u0027 for .NET SDK 8.0)",
      "type": "string"
    },
    "label": {
      "description": "Descriptive label for the SDK version",
      "type": "string"
    },
    "_links": {
      "description": "HAL\u002BJSON links for hypermedia navigation",
      "type": "object",
      "additionalProperties": {
        "description": "HAL\u002BJSON hypermedia link providing navigation to related resources",
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "href": {
            "description": "Absolute URL to the linked resource",
            "type": "string"
          },
          "path": {
            "description": "Absolute path from the repository root (starts with /)",
            "type": [
              "string",
              "null"
            ]
          },
          "title": {
            "description": "Descriptive title for the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "type": {
            "description": "MIME type of the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "templated": {
            "description": "Indicates that the href is a URI template (RFC 6570)",
            "type": [
              "boolean",
              "null"
            ]
          }
        },
        "required": [
          "href"
        ]
      }
    },
    "_embedded": {
      "description": "Embedded SDK feature band entries",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "downloads": {
          "description": "SDK downloads keyed by RID for direct access",
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": {
            "description": "Individual SDK download file for a specific platform",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "name": {
                "description": "File name",
                "type": "string"
              },
              "rid": {
                "description": "Runtime identifier",
                "type": "string"
              },
              "os": {
                "description": "Operating system",
                "type": "string"
              },
              "arch": {
                "description": "Architecture",
                "type": "string"
              },
              "hash_algorithm": {
                "description": "Hash algorithm used for file verification (e.g., \u0027sha512\u0027)",
                "type": "string"
              },
              "_links": {
                "description": "Download and hash links",
                "type": "object",
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "name",
              "rid",
              "os",
              "arch",
              "hash_algorithm",
              "_links"
            ]
          }
        },
        "feature_bands": {
          "description": "List of SDK feature band entries with version information and navigation links",
          "type": "array",
          "items": {
            "description": "Individual SDK feature band entry containing version metadata and navigation links",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "version": {
                "description": "Feature band version (e.g., \u00278.0.1xx\u0027, \u00278.0.4xx\u0027)",
                "type": "string"
              },
              "date": {
                "description": "Release date when this feature band became generally available",
                "type": [
                  "string",
                  "null"
                ],
                "format": "date-time"
              },
              "label": {
                "description": "Descriptive label for the feature band",
                "type": "string"
              },
              "support_phase": {
                "description": "Support phase at time of release (preview, go-live, active, maintenance, eol)",
                "enum": [
                  "preview",
                  "go-live",
                  "active",
                  "maintenance",
                  "eol",
                  null
                ]
              },
              "_links": {
                "description": "HAL\u002BJSON links for navigation to this feature band\u0027s content",
                "type": "object",
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "version",
              "date",
              "label",
              "support_phase",
              "_links"
            ]
          }
        },
        "releases": {
          "description": "List of SDK patch release entries with version information and navigation links",
          "type": "array",
          "items": {
            "description": "Individual SDK release entry containing version metadata and navigation links",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "version": {
                "description": "SDK version (e.g., \u00278.0.100\u0027, \u00279.0.200\u0027)",
                "type": "string"
              },
              "date": {
                "description": "Release date when this SDK version became generally available",
                "type": [
                  "string",
                  "null"
                ],
                "format": "date-time"
              },
              "support_phase": {
                "description": "Support phase at time of release (preview, go-live, active, maintenance, eol)",
                "enum": [
                  "preview",
                  "go-live",
                  "active",
                  "maintenance",
                  "eol",
                  null
                ]
              },
              "_links": {
                "description": "HAL\u002BJSON links for navigation to this SDK release\u0027s content",
                "type": "object",
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "version",
              "date",
              "support_phase",
              "_links"
            ]
          }
        }
      },
      "required": [
        "downloads",
        "feature_bands",
        "releases"
      ]
    },
    "_metadata": {
      "description": "Metadata about when and how this document was generated",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "schema_version": {
          "description": "Version of the schema used for this document",
          "type": "string"
        },
        "generated_on": {
          "description": "ISO 8601 timestamp when this document was generated",
          "type": "string",
          "format": "date-time"
        },
        "generated_by": {
          "description": "Name of the tool or script that generated this document",
          "type": "string"
        }
      },
      "required": [
        "schema_version",
        "generated_on",
        "generated_by"
      ]
    }
  },
  "required": [
    "kind",
    "component",
    "version",
    "label",
    "_links"
  ]
}