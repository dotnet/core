{
  "description": "Index of .NET SDK releases organized by feature bands, supporting navigation from major versions to specific SDK releases",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "kind": {
      "description": "Type of release document, always \u0027sdk-index\u0027 for SDK indexes",
      "enum": [
        "releases-index",
        "major-version-index",
        "patch-version-index",
        "sdk-index",
        "manifest",
        "band",
        "sdk-download",
        "downloads-index",
        "component-download",
        "content",
        "unknown",
        "release-index",
        "index",
        "major-index",
        "major-release-index",
        "patch-index",
        "patch-release-index",
        "major-release",
        "patch-release"
      ]
    },
    "version": {
      "description": "SDK major version (e.g., \u00278.0\u0027, \u00279.0\u0027)",
      "type": "string"
    },
    "title": {
      "description": "Concise title for the document",
      "type": "string"
    },
    "description": {
      "description": "Description of the SDK index",
      "type": "string"
    },
    "latest": {
      "description": "Latest SDK version",
      "type": [
        "string",
        "null"
      ]
    },
    "latest_security": {
      "description": "Latest SDK version that includes security fixes",
      "type": [
        "string",
        "null"
      ]
    },
    "latest_feature_band": {
      "description": "Latest active feature band version (e.g., \u00278.0.4xx\u0027)",
      "type": [
        "string",
        "null"
      ]
    },
    "_links": {
      "description": "HAL\u002BJSON links for hypermedia navigation",
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": {
        "description": "HAL\u002BJSON hypermedia link providing navigation to related resources",
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "href": {
            "description": "Absolute URL to the linked resource",
            "type": "string"
          },
          "title": {
            "description": "Descriptive title for the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "type": {
            "description": "MIME type of the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "templated": {
            "description": "Indicates that the href is a URI template (RFC 6570)",
            "type": [
              "boolean",
              "null"
            ]
          }
        },
        "required": [
          "href"
        ]
      }
    },
    "_embedded": {
      "description": "Embedded SDK feature band entries and releases",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "feature_bands": {
          "description": "List of SDK feature band entries with version information and navigation links",
          "type": "array",
          "items": {
            "description": "Individual SDK feature band entry containing version metadata and navigation links",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "version": {
                "description": "Latest SDK version in this feature band (e.g., \u00279.0.307\u0027)",
                "type": "string"
              },
              "band": {
                "description": "Feature band identifier (e.g., \u00279.0.3xx\u0027)",
                "type": "string"
              },
              "date": {
                "description": "Release date of the latest SDK in this feature band",
                "type": [
                  "string",
                  "null"
                ],
                "format": "date-time"
              },
              "label": {
                "description": "Descriptive label for the feature band",
                "type": [
                  "string",
                  "null"
                ]
              },
              "support_phase": {
                "description": "Support phase (preview, go-live, active, maintenance, eol)",
                "enum": [
                  "preview",
                  "go-live",
                  "active",
                  "maintenance",
                  "eol",
                  null
                ]
              },
              "_links": {
                "description": "HAL\u002BJSON links for navigation to this feature band\u0027s content",
                "type": "object",
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "version",
              "band",
              "date",
              "label",
              "support_phase",
              "_links"
            ]
          }
        }
      },
      "required": [
        "feature_bands"
      ]
    },
    "_metadata": {
      "description": "Metadata about when and how this document was generated",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "schema_version": {
          "description": "Version of the schema used for this document",
          "type": "string"
        },
        "generated_on": {
          "description": "ISO 8601 timestamp when this document was generated",
          "type": "string",
          "format": "date-time"
        },
        "generated_by": {
          "description": "Name of the tool or script that generated this document",
          "type": "string"
        }
      },
      "required": [
        "schema_version",
        "generated_on",
        "generated_by"
      ]
    }
  },
  "required": [
    "kind",
    "version",
    "title",
    "description"
  ]
}