{
  "description": "Component download file with evergreen download links",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "kind": {
      "description": "Type of document, always \u0027component-download\u0027",
      "enum": [
        "releases-index",
        "major-version-index",
        "patch-version-index",
        "sdk-index",
        "manifest",
        "band",
        "sdk-download",
        "downloads-index",
        "component-download",
        "content",
        "unknown",
        "release-index",
        "index",
        "major-index",
        "major-release-index",
        "patch-index",
        "patch-release-index",
        "major-release",
        "patch-release"
      ]
    },
    "component": {
      "description": "Component name (e.g., \u0027runtime\u0027, \u0027aspnetcore\u0027, \u0027windowsdesktop\u0027, \u0027sdk\u0027)",
      "type": "string"
    },
    "version": {
      "description": "Major version (e.g., \u00278.0\u0027, \u00279.0\u0027)",
      "type": "string"
    },
    "title": {
      "description": "Concise title for the document",
      "type": "string"
    },
    "description": {
      "description": "Description of the component download",
      "type": "string"
    },
    "feature_band": {
      "description": "Feature band version for SDK files (e.g., \u00278.0.4xx\u0027)",
      "type": [
        "string",
        "null"
      ]
    },
    "_links": {
      "description": "HAL\u002BJSON links for hypermedia navigation",
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": {
        "description": "HAL\u002BJSON hypermedia link providing navigation to related resources",
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "href": {
            "description": "Absolute URL to the linked resource",
            "type": "string"
          },
          "title": {
            "description": "Descriptive title for the linked resource",
            "type": [
              "string",
              "null"
            ]
          },
          "type": {
            "description": "MIME type of the linked resource. Omit for HAL\u002BJSON (default).",
            "type": [
              "string",
              "null"
            ]
          },
          "templated": {
            "description": "Indicates that the href is a URI template (RFC 6570)",
            "type": [
              "boolean",
              "null"
            ]
          }
        },
        "required": [
          "href"
        ]
      }
    },
    "_embedded": {
      "description": "Embedded download file entries",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "downloads": {
          "description": "Dictionary of downloads keyed by RID (e.g., \u0027linux-x64\u0027, \u0027win-x64\u0027)",
          "type": "object",
          "additionalProperties": {
            "description": "Individual download file for a specific platform",
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "name": {
                "description": "File name",
                "type": "string"
              },
              "rid": {
                "description": "Runtime identifier",
                "type": "string"
              },
              "os": {
                "description": "Operating system",
                "type": "string"
              },
              "arch": {
                "description": "Architecture",
                "type": "string"
              },
              "hash_algorithm": {
                "description": "Hash algorithm used for file verification (e.g., \u0027sha512\u0027)",
                "type": "string"
              },
              "_links": {
                "description": "Download and hash links",
                "type": [
                  "object",
                  "null"
                ],
                "additionalProperties": {
                  "$ref": "#/properties/_links/additionalProperties"
                }
              }
            },
            "required": [
              "name",
              "rid",
              "os",
              "arch",
              "hash_algorithm"
            ]
          }
        }
      },
      "required": [
        "downloads"
      ]
    },
    "_metadata": {
      "description": "Metadata about when and how this document was generated",
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "schema_version": {
          "description": "Version of the schema used for this document",
          "type": "string"
        },
        "generated_on": {
          "description": "ISO 8601 timestamp when this document was generated",
          "type": "string",
          "format": "date-time"
        },
        "generated_by": {
          "description": "Name of the tool or script that generated this document",
          "type": "string"
        }
      },
      "required": [
        "schema_version",
        "generated_on",
        "generated_by"
      ]
    }
  },
  "required": [
    "kind",
    "component",
    "version",
    "title",
    "description"
  ]
}