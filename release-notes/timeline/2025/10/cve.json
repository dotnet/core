  "disclosures": [
    {
      "id": "CVE-2025-55248",
      "problem": ".NET Information Disclosure Vulnerability",
      "description": [
        "An AiTM (Attacker in the middle) attacker may prevent use of TLS between client and SMTP server, forcing client to send data over unencrypted connection."
      ],
      "cvss": {
        "version": "3.1",
        "vector": "CVSS:3.1/AV:N/AC:H/PR:L/UI:R/S:U/C:H/I:N/A:N/E:U/RL:O/RC:C",
        "score": 4.8,
        "severity": "MEDIUM"
      },
      "timeline": {
        "disclosure": {
          "date": "2025-10-14",
          "description": "Publicly disclosed"
        },
        "fixed": {
          "date": "2025-10-14",
          "description": "Fix released"
        }
      },
      "platforms": [
        "all"
      ],
      "architectures": [
        "all"
      ],
      "references": [
        "https://github.com/dotnet/announcements/issues/372"
      ],
      "weakness": "CWE-326",
      "cna": {
        "name": "microsoft",
        "faq": [
          {
            "question": "What type of information could be disclosed by this vulnerability?",
            "answer": "The type of information that could be disclosed if an attacker successfully exploited this vulnerability is Personally Identifiable Information (PII)."
          },
          {
            "question": "According to the CVSS metric, user interaction is required (UI:R) and privileges required is Low (PR:L). What does that mean for this vulnerability?",
            "answer": "An authorized attacker with read/write privileges must send a victim a malicious email, or share the link to a malicious email, and convince them to open it."
          }
        ]
      }
    },
    {
      "id": "CVE-2025-55315",
      "problem": ".NET Security Feature Bypass Vulnerability",
      "description": [
        "Inconsistent interpretation of http requests (\u0027http request/response smuggling\u0027) in ASP.NET Core allows an authorized attacker to bypass a security feature over a network."
      ],
      "cvss": {
        "version": "3.1",
        "vector": "CVSS:3.1/AV:N/AC:L/PR:L/UI:N/S:C/C:H/I:H/A:L/E:U/RL:O/RC:C",
        "score": 9.9,
        "severity": "CRITICAL"
      },
      "timeline": {
        "disclosure": {
          "date": "2025-10-14",
          "description": "Publicly disclosed"
        },
        "fixed": {
          "date": "2025-10-14",
          "description": "Fix released"
        }
      },
      "platforms": [
        "all"
      ],
      "architectures": [
        "all"
      ],
      "references": [
        "https://github.com/dotnet/announcements/issues/371"
      ],
      "weakness": "CWE-444",
      "cna": {
        "name": "microsoft",
        "acknowledgments": [
          "Sid"
        ],
        "faq": [
          {
            "question": "How could an attacker exploit the vulnerability?",
            "answer": "An authenticated attacker could exploit the vulnerability by sending a malicious http request to the web server."
          },
          {
            "question": "According to the CVSS metrics, successful exploitation of this vulnerability could lead to a high loss of confidentiality (C:H), and integrity (I:H) and some loss of availability (A:L). What does that mean for this vulnerability?",
            "answer": "An attacker who successfully exploited this vulnerability could view sensitive information such as other user\u0027s credentials (Confidentiality) and make changes to file contents on the target server (Integrity), and they might be able to force a crash within the server (Availability)."
          },
          {
            "question": "According to the CVSS metric, a successful exploitation could lead to a scope change (S:C). What does this mean for this vulnerability?",
            "answer": "An exploited vulnerability can affect resources beyond the security scope managed by the security authority of the vulnerable component. In this case, the vulnerable component and the impacted component are different and managed by different security authorities."
          },
          {
            "question": "What kind of security feature could be bypassed by successfully exploiting this vulnerability?",
            "answer": "An attacker who successfully exploited this vulnerability could smuggle another HTTP request and bypass front-end security controls or hijack other users\u0027 credentials."
          },
          {
            "question": "This vulnerability is rated as an Important, Security Feature Bypass that is less likely to be exploited. Why is the CVSS score 9.9 out of 10?",
            "answer": "ASP.NET Core is a framework. CVSS scores applications. This mismatch makes scoring ASP.NET challenging. In situations like this, it is Microsoft\u0027s standard practice to score the worst possible case scenario for any application written using ASP.NET Core. \u0026quot;Exploitation less likely\u0026quot; refers to a more typical application which doesn\u0027t go outside the typical ASP.NET Core uses."
          }
        ]
      }
    },
    {
      "id": "CVE-2025-55247",
      "problem": ".NET Denial of Service Vulnerability",
      "description": [
        "A vulnerability exists in .NET where predictable paths for MSBuild\u0027s temporary directories on Linux let another user create the directories ahead of MSBuild, leading to DoS of builds. This only affects .NET on Linux operating systems."
      ],
      "cvss": {
        "version": "3.1",
        "vector": "CVSS:3.1/AV:L/AC:L/PR:L/UI:R/S:U/C:H/I:H/A:H/E:U/RL:O/RC:C",
        "score": 7.3,
        "severity": "HIGH"
      },
      "timeline": {
        "disclosure": {
          "date": "2025-10-14",
          "description": "Publicly disclosed"
        },
        "fixed": {
          "date": "2025-10-14",
          "description": "Fix released"
        }
      },
      "platforms": [
        "linux"
      ],
      "architectures": [
        "all"
      ],
      "references": [
        "https://github.com/dotnet/announcements/issues/370"
      ],
      "mitigation": [
        "Projects which do not utilize the DownloadFile build task are not susceptible to this vulnerability."
      ],
      "weakness": "CWE-59",
      "cna": {
        "name": "microsoft",
        "faq": [
          {
            "question": "What privileges could be gained by an attacker who successfully exploited the vulnerability?",
            "answer": "An attacker who successfully exploited this vulnerability could gain the privileges of the authenticated user."
          },
          {
            "question": "According to the CVSS metric, user interaction is required (UI:R) and privileges required is Low (PR:L). What does that mean for this vulnerability?",
            "answer": "An authenticated attacker could place a malicious file in the core project path and then wait for a user with admin privileges to create or build a .NET project to gain elevated privileges."
          }
        ]
      }
    }
  ],
